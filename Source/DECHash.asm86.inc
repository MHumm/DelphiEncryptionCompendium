                shr  edx, 8
                and  ecx, ebx
                or   eax, ecx

                mov  ecx, dword ptr [edi + ecx*4]
                ror  eax, 8
                xor  ecx, edx
                or   eax, ecx
                ror  eax, 8
                mov  dword ptr [esi + 44], eax

                add  ecx, ebp
                inc  ebp
                and  ecx, ebx
                cmp  ebp, 18
                jnz  @roundsloop

                pop  ebp
                pop  edi
                pop  esi
                pop  ebx
